{
  "name": "@consumemate/backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/app.js",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/database.ts",
    "migration:generate": "npm run typeorm migration:generate src/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "elevenlabs": "^0.17.0",
    "express": "^4.21.0",
    "jsdom": "^24.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "turndown": "^7.2.0",
    "twilio": "^5.3.0",
    "typeorm": "^0.3.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.0",
    "@types/turndown": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.5.4"
  }
}
